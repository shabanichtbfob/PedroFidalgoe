package org.videostore;
// *.dml file name must be the same as specified in the artifactId in the pom.

class Customer {
	String name;
}

relation DomainRootHasCustomer {
	.pt.ist.fenixframework.DomainRoot playsRole root {
		multiplicity 1..1;
	}
	Customer playsRole customer {
		multiplicity 0..*;
	}
}

